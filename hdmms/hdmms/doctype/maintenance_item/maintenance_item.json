{
    "doctype": "DocType",
    "name": "Maintenance Item",
    "module": "HDMMS",
    "istable": 1,
    "editable_grid": 1,
    "fields": [
      {
        "fieldname": "item_code",
        "fieldtype": "Link",
        "label": "Item Code",
        "options": "Item",
        "reqd": 1
      },
      {
        "fieldname": "item_name",
        "fieldtype": "Data",
        "label": "Item Name",
        "fetch_from": "item_code.item_name",
        "read_only": 1
      },
      {
        "fieldname": "description",
        "fieldtype": "Text",
        "label": "Description",
        "fetch_from": "item_code.description",
        "read_only": 1
      },
      {
        "fieldname": "quantity",
        "fieldtype": "Float",
        "label": "Quantity",
        "default": 1,
        "reqd": 1
      },
      {
        "fieldname": "uom",
        "fieldtype": "Link",
        "label": "UOM",
        "options": "UOM",
        "fetch_from": "item_code.stock_uom",
        "read_only": 1
      },
      {
        "fieldname": "rate",
        "fieldtype": "Currency",
        "label": "Rate",
        "fetch_from": "item_code.valuation_rate"
      },
      {
        "fieldname": "amount",
        "fieldtype": "Currency",
        "label": "Amount",
        "read_only": 1
      },
      {
        "fieldname": "warehouse",
        "fieldtype": "Link",
        "label": "Warehouse",
        "options": "Warehouse",
        "default": "Stores - {company}" 
      }
    ]
  }